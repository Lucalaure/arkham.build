{
  "name": "@arkham-build/frontend",
  "private": true,
  "type": "module",
  "engines": {
    "node": "24.x"
  },
  "scripts": {
    "analyze": "npx vite-bundle-visualizer",
    "build": "vite build",
    "check": "tsc --noEmit",
    "dev": "vite",
    "dev:functions": "wrangler pages dev",
    "fmt": "biome check --write",
    "i18n:sync": "node ./scripts/i18n-sync.ts && npm run fmt",
    "i18n:pull": "npx tsx ./scripts/i18n-pull && npm run fmt",
    "icons:export": "mkdir -p public/lucide && npm i lucide-static && cp -R node_modules/lucide-static/icons public/lucide && npm uninstall lucide-static",
    "knip": "npx knip",
    "preview": "vite preview",
    "schema:fan-made-content": "node ./scripts/fan-made-content-schema.ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@dnd-kit/core": "6.3.1",
    "@dnd-kit/sortable": "10.0.0",
    "@dnd-kit/utilities": "3.2.2",
    "@floating-ui/react": "0.27.15",
    "@fontsource-variable/noto-sans": "5.2.7",
    "@fontsource-variable/noto-serif": "5.2.7",
    "@lucide/lab": "0.1.2",
    "@radix-ui/react-checkbox": "1.3.2",
    "@radix-ui/react-collapsible": "1.1.11",
    "@radix-ui/react-radio-group": "1.3.7",
    "@radix-ui/react-scroll-area": "1.2.9",
    "@radix-ui/react-slider": "1.3.5",
    "@radix-ui/react-tabs": "1.1.12",
    "@radix-ui/react-toggle-group": "1.1.10",
    "@tanstack/react-query": "5.84.1",
    "dompurify": "3.2.6",
    "i18next": "25.3.2",
    "i18next-resources-to-backend": "1.2.1",
    "idb-keyval": "6.2.2",
    "lucide-react": "0.536.0",
    "marked": "16.1.2",
    "nanoid": "5.1.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "15.6.1",
    "react-virtuoso": "4.13.0",
    "reselect": "5.1.1",
    "victory": "37.3.6",
    "wouter": "3.7.1",
    "zustand": "5.0.7"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20251225.0",
    "@types/react": "18.3.23",
    "@types/react-dom": "18.3.7",
    "@vitejs/plugin-react": "5.1.2",
    "autoprefixer": "10.4.23",
    "happy-dom": "20.0.11",
    "postcss-extend-rule": "4.0.0",
    "vite": "7.3.0",
    "wrangler": "4.59.1"
  }
}
